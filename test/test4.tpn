/* compile and assign the regular expression to variable rx. */
regex = "[a-z]+";   

/* a function definition */
def gen() {
  len = rand(1, 10);  
  return randrx(len, regex);  // generates random string of len length from regular expression.
}

[T = rand(1,50)];
for range(T-1) {
  ['\n' m = rand(1,10000)];
  for range(m-1) {
    ['\n' gen() ' ' gen()];     // single line looping
  }
}
